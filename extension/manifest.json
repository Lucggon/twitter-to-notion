{

    "manifest_version": 2,
    "name": "modify-page",
    "version": "1.0",
  
    "permissions": [
      "activeTab",
      "contextMenus",
      "webNavigation",
      "<all_urls>"
    ],

    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts":[
      {"matches": ["<all_urls>"],
      "css":["shared.css"]
      }
    ],
    "web_accessible_resources": [
      "./images/notion.png"
    ]
  
  }
  