{

    "manifest_version": 2,
    "name": "modify-page",
    "version": "1.0",
  
    "permissions": [
      "activeTab",
      "contextMenus",
      "webNavigation",
      "<all_urls>"
    ],
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "web_accessible_resources": [
      "index.html"
    ]
  
  }
  